<script>

	const qcount = 115
	let correct_count = 0
	let wrong_count = 0

	const score_correct = document.querySelector('.correct')
	const score_incorrect = document.querySelector('.incorrect')
	const score_results = document.querySelector('.results')

	function updateScore() {
		score_correct.innerText = correct_count
		score_incorrect.innerText = wrong_count
		score_results.innerText = (correct_count / parseFloat(qcount)) * 100
	}

	function onFound() {
		this.classList.add("found")
		correct_count++
		updateScore()
	}

	function onFoundWrong() {
		this.classList.add("oops")
		wrong_count++
		updateScore()
	}

	const answers = document.querySelectorAll('.answer')
	answers.forEach(answer => answer.addEventListener('click', onFound));

	const wrongs = document.querySelectorAll('.wrong')
	wrongs.forEach(wrong => wrong.addEventListener('click', onFoundWrong))


</script>